  },
  "homepage": "https://github.com/shipshapecode/angular-shepherd",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/RobbieTheWagner"
  },
  "dependencies": {
    "shepherd.js": "^14.4.0"
  },
  "peerDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "README.md"
